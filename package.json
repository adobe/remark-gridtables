    "@stylistic/eslint-plugin-js": "2.11.0",
    "c8": "10.1.2",
    "eslint": "9.16.0",
    "husky": "9.1.7",
    "junit-report-builder": "5.1.1",
    "lint-staged": "15.2.10",
    "mdast-builder": "1.1.1",
    "mocha": "11.0.1",
    "mocha-multi-reporters": "1.5.1",
    "rehype-format": "5.0.1",
    "rehype-stringify": "10.0.1",
    "remark-gfm": "4.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.1",
    "remark-stringify": "11.0.0",
    "semantic-release": "24.2.0",
    "unified": "11.0.5",
    "unist-util-visit": "5.0.0"
